{
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "parser":"babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb"
  ],
  "settings":{
    "react": {
      "version":"detect"
    }
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react",
  ],
  "rules": {
    "no-console": "off",
    "react/react-in-jsx-scope":"off"
  }
}
